{
    "extends": [
        "eslint-config-es5"
    ],
    "env": {
        "browser": true
    },
    rules: {
        "indent": [2, 4],
        "func-names": 0,
        "space-before-function-paren": 0
    },
    globals: {
        "google": false,
        "$": false,
        "SnazzyInfoWindow": false,
        "Handlebars": false,
    },
    root: true
}
